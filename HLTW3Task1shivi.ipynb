    "#asks user to enter a number\n",
    "\n",
    "number = int(input(\"Enter a number : \"))\n",
    "\n",
    "#check entered number is even or not\n",
    "result = number % 2 \n",
    "if result == 0  :\n",
    "    print(\"Number is Even\")\n",
    "else :\n",
    "    print(\"Number is odd\")\n"
   ]
